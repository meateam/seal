{
  "name": "katsefet",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run tslint&& npm run build-ts",
    "release": "standard-version",
    "start": "tsc && cross-env NODE_ENV=dev node dist/server.js",
    "test": "npm run tslint&& npm run run_tests",
    "run_tests": "cross-env NODE_ENV=testing mocha --opts ./mocha.opts src/**/*.spec.ts src/specs/spec.runner.ts",
    "prod": "tsc && cross-env NODE_ENV=prod node dist/server.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllKinds/Katsefet.git"
  },
  "author": "Yonatan, Tal & Shahar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AllKinds/Katsefet/issues"
  },
  "nyc": {
    "check-coverage": true,
    "exclude": [],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude":[
      "src/**/*.spec.ts",
      "src/specs/spec.runner.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "homepage": "https://github.com/AllKinds/Katsefet#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.1",
    "@types/mongoose": "^5.0.15",
    "@types/node": "^10.3.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0",
    "ts-node": "^6.2.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.1.4",
    "morgan": "^1.9.0",
    "tslint-microsoft-contrib": "^5.0.3"
  }
}